{
  "name": "chainreactions-user-management",
  "version": "1.0.0",
  "description": "User Management Service for ChainReactions - Supabase Auth Integration",
  "type": "commonjs",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "start": "node -r tsconfig-paths/register dist/app.js",
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/app.ts",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": [
    "user-management",
    "authentication",
    "supabase",
    "chainreactions",
    "microservice"
  ],
  "author": "ChainReactions Team",
  "license": "MIT",
  "dependencies": {
    "@supabase/auth-js": "^2.64.0",
    "@supabase/supabase-js": "^2.87.1",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.7",
    "winston": "^3.11.0",
    "ioredis": "^5.3.2",
    "pg": "^8.11.3",
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.19.26",
    "@types/nodemailer": "^6.4.14",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3",
    "@types/ioredis": "^5.0.0",
    "@types/pg": "^8.10.9"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
